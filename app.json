{
  "expo": {
    "name": "f8-app",
    "slug": "f8-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo/logo.png",
    "scheme": "f8",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": false
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.f8app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.f8app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-image-picker"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
